{
    "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "entry",
            "corejs": 3,
            "targets": {
                "browsers": ["> 0.25%, not dead"]
            }
        }]
    ],
    "plugins": [
        [
        "@babel/plugin-transform-optional-chaining"
        ],
        [
        "@babel/plugin-transform-nullish-coalescing-operator"
        ],
        [
            "transform-react-jsx"
        ],
        ["@babel/plugin-transform-runtime",
            {
                "regenerator": true,
                "corejs": 3
            }
        ]
    ]
}
